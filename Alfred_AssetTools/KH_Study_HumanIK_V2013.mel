global proc string[] JointAimTo(string $jnt, vector $aimVec, vector $upVec) {
    if (!`objExists $jnt`) return {""};

    string $locAim[] = `spaceLocator -n ($jnt+"_x" )`;
    string $locUp[] = `spaceLocator -n  ($jnt+"_y" )`;
    string $tmp[] = `pointConstraint $jnt $locAim[0]`; delete $tmp;
    string $tmp[] = `pointConstraint $jnt $locUp[0]`;  delete $tmp;
    move -r -os -wd 100 0 0 $locAim[0];
    move -r -os -wd 0 100 0 $locUp[0];

    string $tmp[] = `aimConstraint -aimVector ($aimVec.x) ($aimVec.y) ($aimVec.z) -upVector ($upVec.x) ($upVec.y) ($upVec.z) -worldUpType "object" -worldUpObject $locUp[0] $locAim[0] $jnt` ;
    return $tmp;
}

// HIK용 조인트 정의
global proc defineHikSkeleton() {

    global string $gHikJnt[];

    // HIK노드 생성
    characterizationCreate();

    for ($i=0;$i<size($gHikJnt);$i++) {
        if (`objExists $gHikJnt[$i]`) {
            setCharacterObject($gHikJnt[$i], "Character1", $i, 0);
            print ($i+"\t \""+$gHikJnt[$i]+"\" --> \""+$gHikJnt[$i]+"\" map\n");
            continue;
        }
        print ($i+"\t \""+$gHikJnt[$i]+"\" Skip\n");
    }
    //HIKCharacterizationTool;

    // 잠금
    characterizationLock();
    rename "Character1" "HIK";
}

// Daz 정리
{

    // 이름 바꿈
    string $prefix = "";
    renameForHik($prefix, $gDazJnt);

    // Hik
    defineHikSkeleton();
}

// biped 정리
{
    string $sel[] = `ls "Attach_*" "*Dummy*"`;
    delete $sel;

    // 조인트 형태 조정
    for ($jnt in `ls -type joint`) {
        setAttr ($jnt+".radius") 1;
        setAttr ($jnt+".drawStyle") 0;
    }

    // 지글용 조인트 디스플레이 조정
    for ($jnt in `ls -type joint "*jiggle*"`) {
        setAttr ($jnt+".radius") .5;
    }

    // 조인트에서 FBX관련 이름 모두 지움
    string $remove[] = {"CSO2_", "FBXASC032", "BipM"};
    for ($jnt in `ls -type joint`) {
        string $text = $jnt;
        for ($i=0;$i<size($remove);$i++) $text = substituteAllString($text, $remove[$i], "");
        if ($text != "") rename $jnt $text;
    }

    // 쉐이더에서 FBX관련 이름 모두 지움
    string $remove[] = {"FBXASC"};
    for ($node in `ls`) {
        string $rename = $node;
        for ($i=0;$i<size($remove);$i++) $rename = substituteAllString($rename, $remove[$i], "");
        if ($rename != "") {
            catch(`rename $node $rename`);
        }
    }

    // Biped 이름 바꿈
    string $prefix = "";
    renameForHik($prefix, $gBipedJnt);

    // 키들 지움
    select -r Hips ;
    SelectHierarchy;
    delete -channels -unitlessAnimationCurves false  -hierarchy none -controlPoints 0 -shape 1;

    // 조인트 리미트 풀어줌
    for ($jnt in `ls -type joint`) {
        transformLimits -rx 0 0 -erx 0 0 $jnt;
        transformLimits -ry 0 0 -ery 0 0 $jnt;
        transformLimits -rz 0 0 -erz 0 0 $jnt;
    }

    // 그룹 노드들 생성
    string $curve = `curve -d 3 -k 4 -k 4 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12 -k 12 -k 12 -p -1.598721155e-014 -3.061616998e-015 50 -p 13.06019375 -3.061616998e-015 50 -p 39.18058124 -2.39911867e-015 39.18058124 -p 55.40970938 -1.822315034e-030 2.9760663e-014 -p 39.18058124 2.39911867e-015 -39.18058124 -p -6.321585304e-015 3.392866162e-015 -55.40970938 -p -39.18058124 2.39911867e-015 -39.18058124 -p -55.40970938 9.831677308e-031 -1.605634754e-014 -p -39.18058124 -2.39911867e-015 39.18058124 -p -13.06019375 -3.061616998e-015 50 -p -1.776356839e-014 -3.061616998e-015 50`;
    rename $curve "Reference";
    group -n "Mesh" -em ; xform -os -piv 0 0 0;
    setAttr "Mesh.inheritsTransform" 0;
    parent Hips Mesh Reference;

    // Tpose
    makeTPose_Biped();

    // Hik
    defineHikSkeleton();
}
