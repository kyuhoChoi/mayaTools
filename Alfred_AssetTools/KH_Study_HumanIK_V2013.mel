    HIKCharacterControlsTool;

    global proc HIK_tool_AutoSelect_Character() {
        global string $gHIKCurrentCharacter;
        //print "HIK_tool_AutoSelect_Character:call";

        // 선택한 오브젝트의
        string $sel[] = `ls -sl`;
        if (size($sel)==0) return;

        // 네임스페이스를 알아오고
        string $buffer[];
        tokenize $sel[0] ":" $buffer;
        if (!size($buffer)>1) return;
        string $namespace = $buffer[0];

        // 네임스페이스안에 있는 HIKCharacterNode를 알아오고.
        $sel = `ls -type "HIKCharacterNode" ($namespace+":*")`;
        if (size($sel)==0) return;
        $HIKCharacterNode = $sel[0];
        //print ("$HIKCharacterNode = \""+$HIKCharacterNode+"\"\n");

        // 현재 캐릭터 변수에 집어넣고 UI갱신
        $gHIKCurrentCharacter = $HIKCharacterNode;
        hikUpdateCurrentCharacterFromScene();
    }

    // scriptJob Kill
    string $listJobs[] = `scriptJob -listJobs`;
    for ($i=0; $i<size($listJobs); $i++) {
        // print ("["+$i+"] "+$listJobs[$i]+"\n");
        if (`gmatch $listJobs[$i] "*HIK_tool_AutoSelect_Character*"`) {
            string $buffer[];
            tokenize $listJobs[$i] ":" $buffer;
            int $jobNum = int($buffer[0]);

            scriptJob -kill $jobNum -force;
            //print ("scriptJob ("+$jobNum+") Killed ! \n");
        }
    }

    // 스크롤 레이아웃에 스크립트 잡 내용 붙임
    if (`dockControl -q -exists "hikCharacterControlsDock"`)
        scriptJob -parent "hikCharacterControlsDock" -event "SelectionChanged" "HIK_tool_AutoSelect_Character";

