// HIK 플러그인 로드
global proc KH_HIK_PluginLoad() {
    if (!`pluginInfo -query -loaded mayaHIK`) {
        loadPlugin "mayaHIK.mll";
        print "mayaHIK.mll 플러그인 로드\n";
    }
}

// 헤어 뼈 컨스트레인트
global proc constraintHairJnt() {
    select -r Hair_Genesis Hair_hip Hair_abdomen Hair_abdomen2 Hair_chest Hair_neck Hair_head Hair_rCollar Hair_lCollar;
    string $sel[] = `ls -sl`;
    for ($obj in $sel) {
        string $buffer[];
        tokenize $obj "_" $buffer;
        print $buffer[1];
        parentConstraint $buffer[1] $obj;
    }
}

// 조인트 HIK용 이름으로 이름 변경
global proc renameForHik(string $prefix, string $jnt[]) {
    global string $gHikJnt[];

    for ($i=0;$i<size($gHikJnt);$i++) {
        string $joint = $prefix+$jnt[$i];
        if ($joint == $gHikJnt[$i]) continue;
        if (`objExists $gHikJnt[$i]`) rename $gHikJnt[$i] ($gHikJnt[$i]+"#");
        if (`objExists $joint`) rename $joint $gHikJnt[$i];
    }
}

